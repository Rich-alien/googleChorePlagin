{
  "manifest_version": 2,
  "name": "editVK",
  "description": "Изменяем вк под себя",
  "version": "0.0.5",
  "author": "Салават Хакимов",
  "browser_action": {
    "default_title": "удаление блока рекламы и не только",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "js": ["jquery.min.js","main.js"],
      "css": ["style.css"]
    }
  ]
}